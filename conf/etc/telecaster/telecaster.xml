<telecaster>
    <infos>
        <short_name>Pre-Barreau</short_name>
        <name>Pre-Barreau</name>
        <description>La preparation au Barreau de Paris</description>
        <url>http://telecaster-04.parisson.com</url>
        <genre>Vocal</genre>
    </infos>
    <server>
        <host>stream.pre-barreau.com</host>
        <port>8000</port>
        <sourcepassword>source2parisson</sourcepassword>
        <public>0</public>
        <deefuzzer_conf>/etc/telecaster/deefuzzer.xml</deefuzzer_conf>
        <rss>
            <dir>/var/www/rss/</dir>
        </rss>
    </server>
    <media>
        <record>true</record>
        <dir>/home/prebarreau/media</dir>
        <format>mp3</format>
        <bitrate>48</bitrate>
        <channels>1</channels>
        <ogg_quality>3</ogg_quality>
        <samplerate>48000</samplerate>
    </media>
    <jack>
        <input>
        <name>jack_rack:out_1</name>
        </input>
    </jack>
</telecaster>
